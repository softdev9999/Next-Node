{
    "name": "scener",
    "version": "5.1.26",
    "private": true,
    "browser": {
        "child_process": false,
        "fs": false
    },
    "scripts": {
        "dev": "node server",
        "build": "next build",
        "start": "next start -p 8080",
        "analyze": "ANALYZE=true next build",
        "prod": "next build && NODE_ENV=\"production\" && node server",
        "deploy:test": "./scripts/deploy-web test",
        "deploy:beta": "./scripts/deploy-web beta",
        "deploy:prod": "./scripts/deploy-web prod",
        "download:env": "aws s3 sync s3://scener-secure/env ./env",
        "bump": "node ./scripts/bump-version",
        "autotrack": "autotrack -o ./public/js/autotrack.custom.js -p eventTracker,urlChangeTracker,outboundLinkTracker,mediaQueryTracker,cleanUrlTracker",
        "lint": "./scripts/lint",
        "pack-timezones": "node ./scripts/pack-timezones.js",
        "copy:scripts": "cp ./scripts/git/pre-commit ./.git/hooks/ && chmod 777 ./.git/hooks/pre-commit"
    },
    "dependencies": {
        "@date-io/moment": "1.x",
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "@material-ui/pickers": "^3.2.10",
        "@next/bundle-analyzer": "^9.5.0",
        "@zeit/next-source-maps": "^0.0.3",
        "agora-rtc-sdk": "^3.2.1",
        "agora-rtm-sdk": "^1.3.1",
        "autotrack": "^2.4.1",
        "aws-sdk": "^2.709.0",
        "babel-eslint": "10.1.0",
        "bcryptjs": "^2.4.3",
        "broadcast-channel": "^3.1.0",
        "caniuse-lite": "^1.0.30001165",
        "classnames": "^2.2.6",
        "cors": "^2.8.5",
        "crc-32": "1.2.0",
        "crypto": "^1.0.1",
        "cuint": "0.2.2",
        "emoji-picker-react": "^3.2.1",
        "eslint": "^7.6.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-react": "7.20.6",
        "formidable-serverless": "^1.0.3",
        "js-cookie": "^2.2.1",
        "jsonwebtoken": "^8.5.1",
        "jwt-decode": "^2.2.0",
        "libphonenumber-js": "^1.8.5",
        "linkifyjs": "^2.1.9",
        "localforage": "^1.7.4",
        "lru-cache": "^6.0.0",
        "material-ui-phone-number": "^2.2.6",
        "md5": "^2.2.1",
        "moment": "^2.27.0",
        "moment-locales-webpack-plugin": "^1.2.0",
        "moment-timezone": "^0.5.31",
        "monday-sdk-js": "^0.1.0",
        "next": "^9.5.5",
        "nookies": "^2.3.2",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-image-crop": "^8.6.4",
        "react-svg-loader": "^3.0.3",
        "react-virtualized-auto-sizer": "^1.0.2",
        "react-window": "^1.8.5",
        "react-window-infinite-loader": "^1.0.5",
        "sanitize-html": "^1.25.0",
        "sass": "^1.26.9",
        "serverless-mysql": "1.5.4",
        "sharp": "0.25.4",
        "ssl-root-cas": "^1.3.1",
        "swr": "^0.2.3",
        "twilio": "^3.50.0"
    }
}
